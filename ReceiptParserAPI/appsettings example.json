{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=receipt_db.sqlite"
  },
  "GeminiApi": {
    "ApiKey": "YOUR_GEMINI_API_KEY_HERE"
  },
  "JwtSettings": {
    "Secret": "YOUR_SUPER_SECRET_KEY_MUST_BE_AT_LEAST_32_CHARS_LONG",
    "Issuer": "ReceiptParserAPI",
    "Audience": "ReceiptUsers",
    "ExpirationInMinutes": 60
  }
}